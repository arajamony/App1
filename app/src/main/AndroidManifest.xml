<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools" package="com.example.rocka.app1">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_shareapp"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_shareapp_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme" tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".Activities.APP1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".Activities.SecondActivity"/>
        <activity android:name=".Activities.HobbiesActivity"/>
    </application>

</manifest>